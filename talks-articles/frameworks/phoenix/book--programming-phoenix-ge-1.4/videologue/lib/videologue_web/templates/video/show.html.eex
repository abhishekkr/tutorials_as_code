<h1>Show Video</h1>

<ul>

  <li>
    <strong>Url:</strong>
    <a href="<%= @video.url %>"><%= @video.url %></a>
  </li>

  <li>
    <strong>Title:</strong>
    <%= @video.title %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @video.description %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.video_path(@conn, :edit, @video) %></span> |
<span><%= link "Back", to: Routes.video_path(@conn, :index) %></span>
